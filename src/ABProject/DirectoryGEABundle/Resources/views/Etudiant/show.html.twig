{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Etudiant</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nom_etudiant</th>
                <td>{{ entity.nometudiant }}</td>
            </tr>
            <tr>
                <th>Prenom_etudiant</th>
                <td>{{ entity.prenometudiant }}</td>
            </tr>
            <tr>
                <th>Niveau_etude</th>
                <td>{{ entity.niveauetude }}</td>
            </tr>
            <tr>
                <th>Contact</th>
                <td>{{ entity.contact }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ entity.updatedat|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('etudiant') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('etudiant_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
